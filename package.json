{
  "name": "@samfung/md-to-notion",
  "version": "1.0.0",
  "description": "Import markdown files to Notion, with support for Obsidian syntax",
  "author": "Sam Fung",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5amfung/md-to-notion.git"
  },
  "keywords": [
    "notion",
    "markdown",
    "obsidian",
    "import"
  ],
  "license": "MIT",
  "module": "index.ts",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "md-to-notion": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/cli.ts --outdir ./dist --target bun",
    "start": "bun run src/cli.ts",
    "coverage": "bun test --coverage",
    "check": "biome check --fix"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@types/bun": "latest",
    "@types/js-yaml": "^4.0.9"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@notionhq/client": "^5.9.0",
    "js-yaml": "^4.1.1"
  }
}
